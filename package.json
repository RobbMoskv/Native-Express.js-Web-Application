{
  "name": "wepapp",
  "version": "1.0.0",
  "description": "A web application build with Node.js and Express",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "DEBUG=app nodemon app.js",
    "starwin": "set DEBUG=* & nodemon app.js",
    "lint": "./node_modules/.bin/eslint app.js"
  },
  "keywords": [
    "node"
  ],
  "author": "Fabrizio Zihlmann",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.4.1",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2000",
    "env": {
      "NODE_ENT": "development",
      "PORT": "4000"
    }
  }
}
